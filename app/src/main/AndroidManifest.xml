<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.seattleguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WestSeattleActivity"
            android:label="@string/west_seattle_title" />
        <activity
            android:name=".PikePlaceMarketActivity"
            android:label="@string/pike_place_title" />
        <activity
            android:name=".WestlakeCenterActivity"
            android:label="@string/westlake_center_title" />
        <activity
            android:name=".LowerQueenAnneActivity"
            android:label="@string/lower_queen_anne_title" />
        <activity android:name=".EntryActivity"></activity>
    </application>

</manifest>